<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SetCookiesServlet</servlet-name>
  	<display-name>SetCookiesServlet</display-name>
  	<description></description>
  	<servlet-class>nick.pack.SetCookiesServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetCookiesServlet</servlet-name>
  	<display-name>GetCookiesServlet</display-name>
  	<description></description>
  	<servlet-class>nick.pack.GetCookiesServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteCookiesServlet</servlet-name>
  	<display-name>DeleteCookiesServlet</display-name>
  	<description></description>
  	<servlet-class>nick.pack.DeleteCookiesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SetCookiesServlet</servlet-name>
  	<url-pattern>/set-cookies</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetCookiesServlet</servlet-name>
  	<url-pattern>/get-cookies</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteCookiesServlet</servlet-name>
  	<url-pattern>/delete-cookies</url-pattern>
  </servlet-mapping>
</web-app>
